using namespace std;

int main()
{ int a[7]={2,4,9,2,7,4,7};
  for(int i=0;i<7;i++)
   for(int j=i+1;j<7;j++)
     if(a[i]>a[j])
     { a[i]=a[i]*a[j];
	   a[j]=a[i]/a[j];
	   a[i]=a[i]/a[j];
     }
	
  for(int i=0;i<7;i++)
  cout<<a[i];
  return 0;
}

